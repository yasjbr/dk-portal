            table="ADM_APPS_STATUS"
            >
        <id
                name="statId"
                type="java.lang.String"
                column="STAT_ID"
                >
            <generator class="assigned"/>
        </id>
        <property
                name="statAName"
                type="java.lang.String"
                column="STAT_A_NAME"
                not-null="true"
                length="100"
                />
        <property
                name="statLName"
                type="java.lang.String"
                column="STAT_L_NAME"
                length="100"
                />
        <property
                name="userId"
                type="java.lang.String"
                column="USER_ID"
                not-null="true"
                length="20"
                />
        <property
                name="timeStamp"
                type="java.util.Date"
                column="TIME_STAMP"
                not-null="true"
                length="7"
                />
        <property
                name="isActive"
                type="java.lang.String"
                column="IS_ACTIVE"
                length="1"
                />
        <property
                name="notes"
                type="java.lang.String"
                column="NOTES"
                length="200"
                />

        <!-- associations -->

    </class>


</hibernate-mapping>